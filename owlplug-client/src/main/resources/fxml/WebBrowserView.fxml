<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>


<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.owlplug.core.controllers.WebBrowserController">
   <children>
      <HBox prefWidth="200.0">
         <children>
            <JFXTextField fx:id="locationTextField" HBox.hgrow="ALWAYS" />
            <JFXButton fx:id="reloadButton" text="Rd" />
         </children>
      </HBox>
      <StackPane fx:id="browserParent" VBox.vgrow="ALWAYS" />
   </children>
</VBox>
